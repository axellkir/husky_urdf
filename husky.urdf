                    <near>0.01</near>
                    <far>50.0</far>
                </clip>
            </camera>
            <plugin filename="libgazebo_ros_openni_kinect.so" name="kinect_controller">
                <baseline>0.2</baseline>
                <alwaysOn>true</alwaysOn>
                <updateRate>30</updateRate>
